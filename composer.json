{
    "name": "brouzie/gassetic-twig-extension",
    "type": "library",
    "description": "Integration gassetic with Twig and Symfony.",
    "keywords": [
        "assetic",
        "gasetic",
        "gulp"
    ],
    "homepage": "https://github.com/Koc/GasseticTwigExtension",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Konstantin Myakshin",
            "email": "koc-dp@yandex.ru"
        }
    ],
    "archive": {
        "exclude": [
            "/tests"
        ]
    },
    "require": {
        "php": "^5.5.9|^7.0",
        "twig/twig": "~1.30|~2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8",
        "symfony/phpunit-bridge": "~2.8|~3.0",
        "symfony/config": "~2.8|~3.0",
        "symfony/dependency-injection": "~2.8|~3.0",
        "symfony/http-kernel": "~2.8|~3.0",
        "symfony/yaml": "~2.8|~3.0"
    },
    "autoload": {
        "psr-4": {
            "Gassetic\\Bridge": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gassetic\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.2-dev"
        }
    }
}
